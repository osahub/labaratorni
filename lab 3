import sys  # імпортуємо модуль sys, який дає доступ до аргументів командного рядка

def find_pairs(numbers):
    seen = set()  # створюємо множину, яка буде зберігати числа, які ми вже проходили
    pairs = []  # створюємо список, в якому будемо зберігати пари чисел, що складають 10
    for num in numbers:  # проходимо по всіх числах у заданому списку
        target = 10 - num  # обчислюємо число, яке ми повинні знайти, щоб отримати 10
        if target in seen:  # перевіряємо, чи ми вже зустрічали це число раніше
            pairs.append((target, num))  # якщо так, додаємо пару чисел до списку pairs
        else:
            seen.add(num)  # якщо ні, додаємо число до множини seen, щоб ми могли знайти його пізніше
    return pairs  # повертаємо список знайдених пар чисел

if name == "main":
    numbers = list(map(int, sys.argv[1:]))  # беремо всі аргументи командного рядка, крім назви файлу, і перетворюємо їх у список цілих чисел
    pairs = find_pairs(numbers)  # знаходимо всі пари чисел, сума яких дорівнює 10
    for pair in pairs:  # проходимо по всіх знайдених парам чисел
        print(pair[0], "+", pair[1])  # друкуємо пару чисел на екран, додавши знак "+" між ними
